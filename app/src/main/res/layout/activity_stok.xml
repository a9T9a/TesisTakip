<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="340dp"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:layout_marginTop="10dp"
    android:layout_marginBottom="10dp"
    tools:context=".Stok">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Malzeme Giriş"
        android:textColor="#008B9C"
        app:layout_constraintEnd_toEndOf="@+id/imgb_stok_malzeme"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imgb_stok_malzeme"
        app:layout_constraintTop_toBottomOf="@+id/imgb_stok_malzeme" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Aylık  Stok"
        android:textColor="#008B9C"
        app:layout_constraintEnd_toEndOf="@+id/imgb_as"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imgb_as"
        app:layout_constraintTop_toBottomOf="@+id/imgb_as" />

    <TextView
        android:id="@+id/textView104"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Malzeme Stok Liste"
        android:textColor="#008B9C"
        app:layout_constraintEnd_toEndOf="@+id/img_stok_malstok"
        app:layout_constraintStart_toStartOf="@+id/img_stok_malstok"
        app:layout_constraintTop_toBottomOf="@+id/img_stok_malstok" />

    <ImageButton
        android:id="@+id/imgb_stok_garntfiltre"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/search" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Garnet Filtrele"
        android:textColor="#008B9C"
        app:layout_constraintEnd_toEndOf="@+id/imgb_stok_garntfiltre"
        app:layout_constraintStart_toStartOf="@+id/imgb_stok_garntfiltre"
        app:layout_constraintTop_toBottomOf="@+id/imgb_stok_garntfiltre" />

    <ImageButton
        android:id="@+id/imgb_stok_malzeme"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@+id/img_stok_malstok"
        app:layout_constraintStart_toEndOf="@+id/imgb_stok_garntfiltre"
        app:layout_constraintTop_toTopOf="@+id/imgb_stok_garntfiltre"
        app:srcCompat="@drawable/dumptruck" />

    <ImageButton
        android:id="@+id/img_stok_malstok"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginEnd="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgb_stok_malzeme"
        app:srcCompat="@drawable/list" />

    <ImageButton
        android:id="@+id/imgb_as"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="150dp"
        app:layout_constraintEnd_toEndOf="@+id/img_stok_malstok"
        app:layout_constraintTop_toBottomOf="@+id/img_stok_malstok"
        app:srcCompat="@drawable/aylikstok" />

</androidx.constraintlayout.widget.ConstraintLayout>